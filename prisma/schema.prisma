generator client {
  provider   = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userinfo {
  id             String  @id @default(uuid())
  email          String  @db.VarChar(255) @unique
  hashedpassword String  @db.VarChar(255)
  username       String? @db.VarChar(255)
}
